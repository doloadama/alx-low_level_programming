#include "main.h"
#include <stdio.h>
/**
 * rev_string - reverse a string
 * @s: the parameter to be reversed
 * Return: the revesed string
 */

void rev_string(char *s)
{
int i;
int leng;
leng = _strlen(s);

for (i = leng - 1; i <= 0; i--)
{
return (s[i]);
}

/**
  * _strlen - return the lenght
  * @s: the parameter
  * Return: the length of an array
  */

int _strlen(char *s)
{
int i = 0;
while( s[i] != '\0')
{
i++;
}
return (i);

}
